  "description": "onetwotrip redis test",
  "main": "app.js",
  "scripts": {
    "test": "node_modules/mocha/bin/_mocha --ui bdd --recursive ./spec/tests/unit && node_modules/mocha/bin/_mocha --ui bdd --recursive ./spec/tests/functional",
