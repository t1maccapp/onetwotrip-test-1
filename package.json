  "dependencies": {
    "redis": "^2.8.0",
    "uniqid": "^4.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "nsp": "^2.8.1",
    "retire": "^1.4.0",
    "rewire": "^2.5.2",
    "sinon": "^4.0.1",
    "standard": "^10.0.3"
