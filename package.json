    "lint": "node_modules/standard/bin/cmd.js --ignore spec/*",
    "security": "node_modules/retire/bin/retire; node_modules/nsp/bin/nsp check",
    "worker": "node app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t1maccapp/onetwotrip-test-1.git"
